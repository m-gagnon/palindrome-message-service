{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": [
        "eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "node/no-missing-import": "off",
      "node/no-unsupported-features/es-syntax": [
        "error",
        {
          "ignores": ["modules"]
        }
      ],
      "new-cap": "off",
      "no-undef": "off",
      "no-console": "off",
      "jsdoc/require-description": "off",
      "jsdoc/require-returns": "off"
    }
}
